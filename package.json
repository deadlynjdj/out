{
  "name": "instatus-out",
  "version": "1.0.8",
  "description": "Monitor services in your menu bar",
  "main": "main.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --config webpack.common.js --watch",
    "make:linux": "electron-builder --linux",
    "make:macos": "electron-builder --mac --x64 --arm64",
    "make:macos:x64": "electron-builder --mac --x64",
    "make:macos:arm64": "electron-builder --mac --arm64",
    "make:win": "electron-builder --win",
    "prettier:check": "prettier --check 'src/**/*.{js,ts,tsx}'",
    "prettier:apply": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "jest": "jest",
    "test": "yarn jest",
    "start": "electron . -â€“enable-logging"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/instatushq/out.git"
  },
  "keywords": [
    "instatus out",
    "monitoring",
    "services",
    "cloud",
    "out",
    "instatus",
    "electron",
    "atom",
    "shell",
    "app",
    "tray"
  ],
  "author": {
    "name": "Instatus",
    "email": "hello@instatus.com"
  },
  "contributors": [
    {
      "name": "Ali Salah",
      "url": "https://github.com/alisalahio"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/instatushq/out/issues"
  },
  "homepage": "https://instatus.com/out",
  "build": {
    "appId": "com.instatus.out",
    "productName": "Instatus Out",
    "files": [
      "build/**/*",
      "assets/**/*",
      "index.html",
      "LICENSE",
      "main.js",
      "first-run.js",
      "store.js"
    ],
    "directories": {
      "buildResources": "buildResources",
      "output": "dist"
    },
    "mac": {
      "extendInfo": {
        "LSUIElement": "true"
      },
      "target": [
        "dmg",
        "zip"
      ],
      "artifactName": "${productName}-${version}-${os}.${ext}",
      "icon": "assets/images/icon.icns",
      "category": "public.app-category.developer-tools",
      "identity": "Instatus, Inc. (BRJY56MYNC)",
      "gatekeeperAssess": false,
      "darkModeSupport": true,
      "hardenedRuntime": true,
      "entitlements": "./entitlements/entitlements.mac.plist",
      "entitlementsInherit": "./entitlements/entitlements.mac.plist",
      "publish": [
        "github"
      ]
    },
    "dmg": {
      "icon": "assets/images/icon.icns",
      "sign": true
    },
    "win": {
      "target": "nsis",
      "icon": "assets/images/app-icon.ico"
    },
    "nsis": {
      "oneClick": false
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Development",
      "maintainer": "Instatus"
    },
    "afterSign": "scripts/notarize.js"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "axios": "1.10.0",
    "@primer/octicons-react": "^19.15.2",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.6.2",
    "final-form": "^5.0.0",
    "js-search": "^2.0.1",
    "lodash": "^4.17.21",
    "menubar": "^9.5.1",
    "nprogress": "=0.2.0",
    "react": "=19.1.0",
    "react-dom": "=19.1.0",
    "react-emojione": "=5.0.1",
    "react-final-form": "^7.0.0",
    "react-router-dom": "^5.3.4",
    "react-transition-group": "^4.4.4",
    "react-typist": "^2.0.5"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.18",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.12",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "autoprefixer": "^10.4.21",
    "babel-eslint": "^10.1.0",
    "css-loader": "^7.1.2",
    "electron": "^12.0.7",
    "electron-builder": "^26.0.12",
    "electron-notarize": "^1.2.2",
    "eslint": "9.29.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-wesbos": "4.3.2",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "jest": "^30.0.2",
    "nock": "^14.0.5",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.0",
    "react-test-renderer": "=19.1.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.5",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  }
}
